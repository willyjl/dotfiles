if [ -x  "$(command -v nvim)" ]; then
  alias vi=/usr/bin/nvim
fi
alias repo="cd ~/repo"
alias usmall="cd ~/repo/us-ui-mall"
alias startmall="sudo service nginx start; sudo service redis-server start;"
alias gpou="git push origin \"$(git rev-parse --abbrev-ref HEAD)\" -u"
alias gpof="git push origin \"$(git rev-parse --abbrev-ref HEAD)\" --force-with-lease"
alias gpof!="git push origin \"$(git rev-parse --abbrev-ref HEAD)\" --force"
